{
  "default": {
    "backgroundColor": "#fff",
    "backgroundOpacity": 0.6,
    "Frame": {
      "layerType": "fillLayer",
      // layer类型
      "renderer": {
        "type": "simple",
        "default": {
          "fillColor": "#fff",
          // 填充色
          "fillOpacity": 0.6,
          //填充透明度
